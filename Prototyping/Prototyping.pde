void setup() {
  size(400, 400); // Set the canvas size
  background(255); // Set the background to white
}

void draw() {
  fill(239,147,27); // Set the fill color to blue (RGB: 0, 0, 255)
  textSize(32); // Set the text size
  text("I give you my life...", 50, 200); // Draw the text at position (50, 200)
  fill(215,37,84);
  text("So give me yours.", 50, 250);
}
